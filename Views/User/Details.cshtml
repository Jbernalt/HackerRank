@model HackerRank.Models.Users.User;

@{
    ViewData["Title"] = @Html.DisplayFor(model => model.UserName);
}

<h1>Details</h1>

<div>
    <h4>Achievement</h4>
    <hr />
    <br />
    <dl class="col">
        <dt class="col-sm-2">
            @Html.DisplayName("User")
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UserName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateCreated)
        </dd>
    </dl>
</div>